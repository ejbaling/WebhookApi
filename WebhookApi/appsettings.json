{
  "RabbitMQ": {
    "HostName": "localhost",
    "QueueName": "gmail-notifications"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Telegram": {
    "BotToken": "YOUR_TELEGRAM_BOT_TOKEN",
    "ChatId": "YOUR_TELEGRAM_USER_ID"
  },
  "SmsGateway": {
    "Url": "http://<ip-address>:8080/message",
    "User": "user",
    "Password": "password"
  },
  "Google": {
    "CredentialsPath": "./credentials.json",
    "ClientId": "YOUR_GOOGLE_CLIENT_ID",
    "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET",
    "RefreshToken": "YOUR_GOOGLE_REFRESH_TOKEN"
  },
  "BlackListedPhoneNumbers": ["GLOBEREWARD", "222", "8080"]
}
