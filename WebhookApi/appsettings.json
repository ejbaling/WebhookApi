{
  "RabbitMQ": {
    "HostName": "localhost",
    "QueueName": "gmail-notifications"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient": "Warning",
      "System.Net.Http.HttpClient.TailscaleMonitorService": "Warning"
    }
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "System.Net.Http.HttpClient": "Warning",
        "System.Net.Http.HttpClient.TailscaleMonitorService": "Warning"
      }
    },
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] ({MachineName}/{ThreadId}) {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Telegram": {
    "BotToken": "YOUR_TELEGRAM_BOT_TOKEN",
    "ChatId": "YOUR_TELEGRAM_USER_ID",
    "AiBotToken": "YOUR_TELEGRAM_BOT_TOKEN",
    "AiChatId": "YOUR_TELEGRAM_GROUPCHAT_ID",
    "AdminBotToken": "YOUR_ADMIN_TELEGRAM_BOT_TOKEN",
    "AllowedUserId": "YOUR_TELEGRAM_USER_ID"
  },
  "AI": {
    "ApiKey": "YOUR_OPENAI_API_KEY",
    "Model": "gpt-4o-mini"
  },
  "SmsGateway": {
    "Url": "http://<ip-address>:8080/message",
    "User": "user",
    "Password": "password",
    "ForwardToNumbers": []
  },
  "Asterisk": {
    "Host": "ip-address",
    "Port": "5038",
    "User": "",
    "Secret": "",
    "Extensions": ["105", "500"]
  },
  "Google": {
    "CredentialsPath": "./credentials.json",
    "ClientId": "YOUR_GOOGLE_CLIENT_ID",
    "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET",
    "RefreshToken": "YOUR_GOOGLE_REFRESH_TOKEN"
  },
  "BlackListedPhoneNumbers": [
    "GLOBE",
    "GLOBEREWARD",
    "222",
    "8080",
    "NDRRMC",
    "22565917",
    "Duxton"
  ],
  "Tailscale": {
    "ApiKey": "YOUR_TAILSCALE_API_KEY",
    "Tailnet": "YOUR_TAILNET",
    "Enabled": false,
    "PollIntervalSeconds": 60,
    "OfflineMinutesThreshold": 5,
    "AllowedDeviceNames": ["iloilo-vpn-router, samsung-sm-a045f-1"]
  },
  "GmailNotification": {
    "Enabled": false
  },
  "AiTelegramService": {
    "Enabled": false
  },
  "TelegramReceiverService": {
    "Enabled": false
  }
}
